<div class="panel_wrapper">
    <div class="panel">
        <div id="panel_title">
            Prisijungimas
        </div>
        <fieldset class="form" id="prisijungimas">
            <form action="{{ path("fos_user_security_check") }}" method="post">
                <input type="hidden" name="_csrf_token" value="{{ token }}"/>

                <div>
                    <label for="username">Vardas</label>
                    <input id="username" type="text" name="_username" size="30" required="required">
                </div>
                <div>
                    <label for="password">Slapta≈æodis</label>
                    <input id="password" type="password" name="_password" size="30" required="required">
                </div>
                <div id="remember">
                    <label for="remember_me">Prisiminti</label>
                    <input type="checkbox" name="_remember_me" value="on"/>
                </div>
                <div>
                    <button type="submit" id="_submit" name="_submit">Prisijungti</button>
                </div>
            </form>
        </fieldset>
    </div>
</div>
